{
  "name": "lang-editor",
  "displayName": "Minecraft .lang Editor",
  "description": "A short description of the extension",
  "publisher": "adamraichu",
  "version": "1.0.0",
  "icon": "i18n.png",
  "author": {
    "name": "AdamRaichu"
  },
  "categories": [
    "Programming Languages",
    "Visualization"
  ],
  "repository": {
    "url": "https://github.com/AdamRaichu/vscode-lang-editor"
  },
  "browser": "/dist/main.js",
  "main": "/dist/main.js",
  "engines": {
    "vscode": "^1.63.0"
  },
  "activationEvents": [
    "onCustomEditor:langEditor.LangEdit",
    "*"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "langEditor.LangEdit",
        "displayName": "Lang Editor",
        "selector": [
          {
            "filenamePattern": "**/texts/*.lang"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "build": "webpack"
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0",
    "@vscode/vsce": "^2.16.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}